<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<navigation-rule>
		<from-view-id>/AddTSStep1.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{addTimeSlotBean.createTimeSlot()}</from-action>
			<to-view-id>/AddTSStep2.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/TimeSlot.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>DETTAGLIO</from-outcome>
			<to-view-id>/DettaglioTS.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Storico.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>DETTAGLIO</from-outcome>
			<to-view-id>/DettaglioStorico.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/TimeSlot.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>STEP1</from-outcome>
			<to-view-id>/AddTSStep1.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/DettaglioTS.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>LISTA</from-outcome>
			<to-view-id>/TimeSlot.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/DaProcessare.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{daProcessareBean.daProcessare_BottoneRichieste(item.timeSlot, item.idTimeSlot)}</from-action>
			<to-view-id>/DaProcessare_Richieste.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/DaProcessare_Richieste.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{daProcessareBean.daProcessare_Richieste_BottoneDettaglio(item.idMission)}</from-action>
			<to-view-id>/DaProcessare_Richieste_Dettaglio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/DaProcessare.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{daProcessareBean.daProcessare_BottoneSimulazione(item.timeSlot, item.idTimeSlot)}</from-action>
			<to-view-id>/DaProcessare_Simulazione.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Storico.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{storicoBean.storico_BottoneDettaglio(item.dateMission, item.idTimeSlot)}</from-action>
			<to-view-id>/Storico_Dettaglio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/TimeSlot.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>TIMESLOT_DETTAGLIO</from-outcome>
			<to-view-id>/TimeSlot_Dettaglio.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/AddTSStep2.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{addTimeSlotBean.addTSStep2_BottoneSetta(item.idPG)}</from-action>
			<to-view-id>/AddTSStep3.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/AddTSStep3.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{addTimeSlotBean.createParameterTS()}</from-action>
			<to-view-id>/AddTSStep2_1.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/AddTSStep2_1.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{addTimeSlotBean.addTSStep2_BottoneSetta(item.idPG)}</from-action>
			<to-view-id>/AddTSStep3.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/AddTSStep2_1.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>STEP4</from-outcome>
			<to-view-id>/AddTSStep4.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/AddTSStep2.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>STEP4</from-outcome>
			<to-view-id>/AddTSStep4.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/AddTSStep4.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{addTimeSlotBean.createPrice()}</from-action>
			<to-view-id>/TimeSlot.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Opzioni.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>AGGPG</from-outcome>
			<to-view-id>/Opzioni_AddPG.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/Opzioni_AddPG.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{opzioniBean.addParameterGen()}</from-action>
			<to-view-id>/Opzioni.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/TimeSlot.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{timeSlotBean.timeSlot_BottoneVisualizza(item.idTS)}</from-action>
			<to-view-id>/TimeSlot_Visualizza.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/TimeSlot_Visualizza.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>TORNATS</from-outcome>
			<to-view-id>/TimeSlot.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<managed-bean>
		<managed-bean-name>timeSlotBean</managed-bean-name>
		<managed-bean-class>it.vige.greenarea.cl.admin.bean.TimeSlotBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>parameterGenBean</managed-bean-name>
		<managed-bean-class>it.vige.greenarea.cl.admin.bean.ParameterGenBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>daProcessareBean</managed-bean-name>
		<managed-bean-class>it.vige.greenarea.cl.admin.bean.DaProcessareBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>opzioniBean</managed-bean-name>
		<managed-bean-class>it.vige.greenarea.cl.admin.bean.OpzioniBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>storicoBean</managed-bean-name>
		<managed-bean-class>it.vige.greenarea.cl.admin.bean.StoricoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>calendarBean</managed-bean-name>
		<managed-bean-class>it.vige.greenarea.cl.admin.bean.CalendarBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>calendarBean1</managed-bean-name>
		<managed-bean-class>it.vige.greenarea.cl.admin.bean.CalendarBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<application>
		<resource-bundle>
			<base-name>/greenareamessages</base-name>
			<var>messages</var>
		</resource-bundle>
		<locale-config>
			<default-locale>it_IT</default-locale>
			<supported-locale>it_IT</supported-locale>
			<supported-locale>en_US</supported-locale>
		</locale-config>
	</application>
</faces-config>
